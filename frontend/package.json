    "rater-js": "^1.0.1",
    "sweetalert2": "^11.23.0",
    "toastify-js": "^1.12.0",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
